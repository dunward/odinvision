<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <FontFamily x:Key="Stereofunk">pack://application;,,,/Fonts/#Stereofunk Display</FontFamily>

    <Style TargetType="{x:Type Control}" x:Key="Titlebar">
        <Setter Property="FontFamily" Value="{StaticResource Stereofunk}"/>
    </Style>
    
    

</ResourceDictionary>